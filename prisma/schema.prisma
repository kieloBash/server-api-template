datasource db {
  provider = "postgresql"
  schemas  = "financial"
}

generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?

  @@schema("financial")
}
